# TwoDimenArray
package jrJava.twoDimenArray;

public class BasicPractice {

	
	public static void main(String[] args) {
		
		int[][] nums = new int [5][8];
		
		for(int i = 0; i<nums.length; i++){
			for(int j= 0; j<nums[i].length; j++){
				nums[i][j] = 10*i+j;
				
			
			}
			
		}
		System.out.println(nums[3][2]);
	}

}
